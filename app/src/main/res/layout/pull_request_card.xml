<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                                    xmlns:android="http://schemas.android.com/apk/res/android"
                                    xmlns:tools="http://schemas.android.com/tools"
                                    android:id="@+id/card_view"
                                    android:layout_gravity="center"
                                    android:layout_width="match_parent"
                                    android:layout_height="130dp"
                                    android:layout_marginStart="8dp"
                                    android:layout_marginEnd="8dp"
                                    android:layout_marginTop="0dp"
                                    android:layout_marginBottom="8dp"
                                    card_view:cardCornerRadius="2dp">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <TextView
            android:layout_width="0dp"
            android:layout_height="53dp"
            android:paddingEnd="8dp"
            android:paddingStart="0dp"
            android:gravity="center|start"
            tools:text="Szczyglowski Lukasz-KXCQ63"
            android:maxLines="2"
            android:ellipsize="end"
            android:textColor="@color/secondary_text"
            android:textSize="16sp"
            tools:layout_conversion_absoluteX="0dp"
            tools:layout_conversion_absoluteY="0dp"
            tools:layout_conversion_absoluteWidth="395dp"
            tools:layout_conversion_absoluteHeight="64dp"
            android:id="@+id/reviewAuthor"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintRight_toLeftOf="@+id/firstReviewer"
            android:layout_marginTop="0dp"
            card_view:layout_constraintHorizontal_bias="0.0"
            card_view:layout_constraintLeft_toRightOf="@+id/authorAvatar"/>

        <TextView
            android:id="@+id/reviewTitle"
            tools:text="[IMW_MOBILEAPP-898] Location Service randomly crashed"
            android:textSize="16sp"
            android:textColor="@color/primary_text"
            android:layout_height="wrap_content"
            android:lines="2"
            android:ellipsize="end"
            tools:layout_conversion_absoluteX="0dp"
            tools:layout_conversion_absoluteY="64dp"
            tools:layout_conversion_absoluteWidth="395dp"
            tools:layout_conversion_absoluteHeight="16dp"
            card_view:layout_constraintRight_toRightOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/reviewAuthor"
            android:layout_width="0dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            card_view:layout_constraintLeft_toLeftOf="parent"/>

        <ImageView
            android:layout_width="48dp"
            android:layout_height="53dp"
            android:padding="8dp"
            tools:src="@drawable/avatar"
            android:contentDescription="@string/reviewer_avatar_content_desc"
            tools:layout_conversion_absoluteX="0dp"
            tools:layout_conversion_absoluteY="89dp"
            tools:layout_conversion_absoluteWidth="48dp"
            tools:layout_conversion_absoluteHeight="53dp"
            android:id="@+id/firstReviewer"
            card_view:layout_constraintRight_toLeftOf="@+id/secondReviewer"
            card_view:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:layout_width="48dp"
            android:layout_height="53dp"
            android:padding="8dp"
            tools:src="@drawable/avatar"
            android:contentDescription="@string/reviewer_avatar_content_desc"
            tools:layout_conversion_absoluteX="0dp"
            tools:layout_conversion_absoluteY="89dp"
            tools:layout_conversion_absoluteWidth="48dp"
            tools:layout_conversion_absoluteHeight="53dp"
            android:id="@+id/authorAvatar"
            card_view:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="0dp"
            card_view:layout_constraintLeft_toLeftOf="parent"/>

        <ImageView
            android:layout_width="48dp"
            android:layout_height="53dp"
            android:padding="8dp"
            tools:src="@drawable/avatar"
            android:contentDescription="@string/reviewer_avatar_content_desc"
            tools:layout_conversion_absoluteX="0dp"
            tools:layout_conversion_absoluteY="89dp"
            tools:layout_conversion_absoluteWidth="48dp"
            tools:layout_conversion_absoluteHeight="53dp"
            android:id="@+id/secondReviewer"
            card_view:layout_constraintRight_toLeftOf="@+id/thirdReviewer"
            card_view:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:layout_width="48dp"
            android:layout_height="53dp"
            android:padding="8dp"
            tools:src="@drawable/avatar"
            android:contentDescription="@string/reviewer_avatar_content_desc"
            tools:layout_conversion_absoluteX="48dp"
            tools:layout_conversion_absoluteY="89dp"
            tools:layout_conversion_absoluteWidth="48dp"
            tools:layout_conversion_absoluteHeight="53dp"
            android:id="@+id/thirdReviewer"
            card_view:layout_constraintRight_toLeftOf="@+id/fourthReviewer"
            card_view:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:layout_width="48dp"
            android:layout_height="53dp"
            android:padding="8dp"
            tools:src="@drawable/avatar"
            android:contentDescription="@string/reviewer_avatar_content_desc"
            tools:layout_conversion_absoluteX="96dp"
            tools:layout_conversion_absoluteY="89dp"
            tools:layout_conversion_absoluteWidth="48dp"
            tools:layout_conversion_absoluteHeight="53dp"
            android:id="@+id/fourthReviewer"
            card_view:layout_constraintRight_toRightOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/reviewBranch"
            android:textColor="@color/secondary_text"
            tools:text="lukasz/imw_mobileapp-880/best_fix_ever_done"
            android:ellipsize="end"
            android:lines="1"
            android:maxLength="30"
            android:minWidth="80dp"
            android:layout_marginStart="8dp"
            card_view:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="7dp"
            card_view:layout_constraintTop_toBottomOf="@+id/reviewTitle"
            android:layout_marginLeft="8dp"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/secondary_text"
            android:id="@+id/targetBranch"
            tools:text="develop"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="7dp"
            android:ellipsize="end"
            android:lines="1"
            android:minWidth="80dp"
            card_view:layout_constraintTop_toBottomOf="@+id/reviewTitle"
            card_view:layout_constraintLeft_toRightOf="@+id/imageView11"
            android:layout_marginRight="8dp"
            card_view:layout_constraintRight_toRightOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"/>

        <ImageView
            android:layout_width="27dp"
            android:layout_height="17dp"
            card_view:srcCompat="@drawable/ic_trending_flat_black_24dp"
            android:id="@+id/imageView11"
            card_view:layout_constraintLeft_toRightOf="@+id/reviewBranch"
            android:layout_marginTop="8dp"
            android:layout_marginStart="8dp"
            card_view:layout_constraintTop_toBottomOf="@+id/reviewTitle"/>


    </android.support.constraint.ConstraintLayout>


</android.support.v7.widget.CardView>